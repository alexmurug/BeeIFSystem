    <script>
// request permission on page load
document.addEventListener('DOMContentLoaded', function () {
  if (Notification.permission !== "granted")
    Notification.requestPermission();
});

//setInterval(function(){Play()},3000)
function delay(ms) {
        var cur_d = new Date();
        var cur_ticks = cur_d.getTime();
        var ms_passed = 0;
        while(ms_passed < ms) {
            var d = new Date();  // Possible memory leak?
            var ticks = d.getTime();
            ms_passed = ticks - cur_ticks;
            // d = null;  // Prevent memory leak?
        }
    }
setTimeout(function(){Cutremur()}, 10000);
setTimeout(function(){notifyMe()}, 18000);



function notifyMe() {
  if (!Notification) {
    alert('Desktop notifications not available in your browser. Try Chromium.');
    return;
  }

  if (Notification.permission !== "granted")
    Notification.requestPermission();
  else {
    var notification = new Notification('Accident grav pe Viaduct', {
        icon: "/Content/img/logo.png",
      body: "3 Morţi şi 2 Răniţi!",
    });

    notification.onclick = function () {
      window.open("#");
    };

  }


  document.getElementById('Accident').play();


}

function Cutremur() {
  if (!Notification) {
    alert('Desktop notifications not available in your browser. Try Chromium.');
    return;
  }

  if (Notification.permission !== "granted")
    Notification.requestPermission();
  else {
    var notification = new Notification('Un cutremur mic, cu magnitudinea de moment Mw 4.6', {
        icon: "/Content/img/logo.png",
      body: "a avut loc in Honshu, Japonia pe data de 15-05-2016, ora 01:10:45 (UTC). Cutremurul a fost localizat la o adancime de 59 km. !",
    });

    notification.onclick = function () {
      window.open("#");
    };

  }


  document.getElementById('alertSound').play();


}
    </script>


    <!-- ChartJS 1.0.1 -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.1.3/Chart.js"></script>



</head>
<audio id="alertSound" src="~/Content/audio/alarm.mp3" preload="auto"></audio>
<audio id="Accident" src="~/Content/audio/Oldalarm.mp3" preload="auto"></audio>
<body data-spy="scroll" data-offset="0" data-target="#navigation">

    <!-- Fixed navbar -->
    <div id="navigation" class="navbar navbar-default navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <img class="navbar-brand" src="~/Content/img/logo.png" alt="BI System"><a href="#home" class="smoothScroll navbar-brand"><b>BeeIF System</b></a>
                <!--<a class="navbar-brand" href="#"><img src="~/Content/img/intro02.png" class?" alt=""><b>Alert System</b></a> -->
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li class="active"><a href="#home" class="smoothScroll">Acasă</a></li>
                    <li><a href="#desc" class="smoothScroll">Info</a></li>
                    <li><a href="#showcase" class="smoothScroll">Vizualizează</a></li>
                    <li><a href="#contact" class="smoothScroll">Postează</a></li>
                </ul>
                <ul class="nav navbar-nav navbar-right">
                    <li><a href="#">Autentificare</a></li>
                    <li><a href="#">Înregistare</a></li>
                </ul>
            </div><!--/.nav-collapse -->
        </div>
    </div>

    <section id="home" name="home"></section>
    @RenderBody()
        <div id="c">
            <div class="container">
                <p>
                    <strong>&copy; @DateTime.Now.Year <a href="/">BeeIF System</a>.</strong> Created by <a>Amazing people</a>
                </p>
            </div>
        </div>
    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="~/Content/BeeIF/JS/bootstrap.min.js" type="text/javascript"></script>





